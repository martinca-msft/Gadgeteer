<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- == == == == == == == == == == Gadgeteer Mark == == == == == == == == == == -->
    
    <SolidColorBrush x:Key="GadgeteerMarkBackgroundBrush" Color="Black" />
    <SolidColorBrush x:Key="GadgeteerMarkForegroundBrush" Color="White" />

    <DrawingImage x:Key="GadgeteerMarkImage">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <GeometryDrawing Brush="{StaticResource GadgeteerMarkBackgroundBrush}" Geometry="M 0,0 V7 H7 V0 Z" />
                <GeometryDrawing Brush="{StaticResource GadgeteerMarkForegroundBrush}" Geometry="M 1,1 V6 H6 V3 H3 V4 H5 V5 H2 V2 H6 V1 Z" />
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

    <DrawingImage x:Key="GadgeteerMarkImageInverted">
        <DrawingImage.Drawing>
            <DrawingGroup>
                <GeometryDrawing Brush="{StaticResource GadgeteerMarkForegroundBrush}" Geometry="M 0,0 V7 H7 V0 Z" />
                <GeometryDrawing Brush="{StaticResource GadgeteerMarkBackgroundBrush}" Geometry="M 1,1 V6 H6 V3 H3 V4 H5 V5 H2 V2 H6 V1 Z" />
            </DrawingGroup>
        </DrawingImage.Drawing>
    </DrawingImage>

</ResourceDictionary>